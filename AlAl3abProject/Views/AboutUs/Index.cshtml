@{
    Layout = "_Layout1";
}
@model IndexHomePageModel
<!-- Start of Main -->
<main class="main">
    <!-- End of Breadcrumb -->
    <!-- Start of Page Content -->
    <div class="banner parallax"
         data-parallax-options="{'speed': 10, 'parallaxHeight': '200%', 'offset': -99}"
         data-image-src="Benzoo/assets/images/15.jpg"
         style="background-color: rgb(146, 146, 148); position: relative; overflow: hidden;height: 550px;">
        <div class="parallax-background"
             style="background-image: url(&quot;assets/images/15.jpg&quot;); background-size: cover; position: absolute; top: 0px; left: 0px; width: 100%; height: 200%; transform: translate3d(0px, -141.926px, 0px); background-position: center center ;">
        </div>
        <div class="container">
            <div class="row pt-20">
                <div class="col-lg-8 banner-content">
                    <h4 class="text-white">@(AlAl3abProject.Resources.ResWebsite.Culture.Name == "en" ? Model.elelmentAboutUs.FirstTitleEn1 : Model.elelmentAboutUs.FirstTitle)</h4>
                    <h2 class="text-gold">
                        @(AlAl3abProject.Resources.ResWebsite.Culture.Name == "en" ? Model.elelmentAboutUs.SecondTitleEn1 : Model.elelmentAboutUs.SecondTitle)
                    </h2>
                    <p class="text-semigold">
                        @(AlAl3abProject.Resources.ResWebsite.Culture.Name == "en" ? Model.elelmentAboutUs.FirstTextEn1 : Model.elelmentAboutUs.FirstText)
                    </p>
                </div>
                <div class="col-lg-4"></div>
            </div>
        </div>
    </div>

    <section class="boost-section pt-10 pb-10">
        <div class="container mt-10 mb-9">
            <div class="row align-items-center mb-10">
                <div class="col-md-6 pl-lg-8 mb-8">
                    <h4 class="title text-left">@(AlAl3abProject.Resources.ResWebsite.Culture.Name == "en" ? Model.elelmentAboutUs.ThirdTtitleEn1 : Model.elelmentAboutUs.ThirdTtitle)</h4>
                    <p class="mb-6 text-white">
                        @(AlAl3abProject.Resources.ResWebsite.Culture.Name == "en" ? Model.elelmentAboutUs.SecondTextEn1 : Model.elelmentAboutUs.SecondText)
                    </p>
                    <p class="mb-6 text-white">
                        @(AlAl3abProject.Resources.ResWebsite.Culture.Name == "en" ? Model.elelmentAboutUs.CreatedByEn1 : Model.elelmentAboutUs.CreatedBy)
                    </p>
                    <p class="mb-6 text-white">
                        @(AlAl3abProject.Resources.ResWebsite.Culture.Name == "en" ? Model.elelmentAboutUs.UpdatedByEn : Model.elelmentAboutUs.UpdatedBy)
                    </p>
                </div>
                <div class="col-md-6 mb-8">
                    <figure class="br-lg">
                        <img src="~/Benzoo/assets/images/about-us.png" alt="Banner" width="610" height="450">
                    </figure>
                </div>

            </div>
        </div>
    </section>


</main>
<!-- End of Main -->
